{
	"name": "CopyPipeline_zg9",
	"properties": {
		"activities": [
			{
				"name": "ForEach_zg9",
				"type": "ForEach",
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_zg9",
							"type": "Copy",
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.tableName}"
								},
								{
									"name": "Destination",
									"value": "jsonperf/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "SapTableSource",
									"batchSize": {
										"value": "@item().copySource.batchSize",
										"type": "Expression"
									},
									"rowCount": {
										"value": "@item().copySource.rowCount",
										"type": "Expression"
									},
									"rfcTableFields": {
										"value": "@item().copySource.rfcTableFields",
										"type": "Expression"
									},
									"rfcTableOptions": {
										"value": "@item().copySource.rfcTableOptions",
										"type": "Expression"
									}
								},
								"sink": {
									"type": "BlobSink"
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_zg9",
									"type": "DatasetReference",
									"parameters": {
										"cw_tableName": "@item().source.tableName"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_zg9",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"tableName": "/BIC/6RSDRSIS01"
						},
						"destination": {
							"fileName": "BIC6RSDRSIS01.txt"
						},
						"copySource": {
							"batchSize": 10,
							"rowCount": 11,
							"rfcTableFields": "DATETO",
							"rfcTableOptions": "DATETO EQ '99991231'"
						}
					},
					{
						"source": {
							"tableName": "/BIC/6RSDRSIS03"
						},
						"destination": {
							"fileName": "BIC6RSDRSIS03.txt"
						},
						"copySource": {
							"batchSize": null,
							"rowCount": null,
							"rfcTableFields": null,
							"rfcTableOptions": null
						}
					}
				]
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}
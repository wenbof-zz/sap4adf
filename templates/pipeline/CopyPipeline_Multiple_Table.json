{
	"name": "CopyPipeline_Multiple_Table",
	"properties": {
		"activities": [
			{
				"name": "ForEach_271",
				"type": "ForEach",
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_271",
							"type": "Copy",
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.openHubDestinationName}"
								},
								{
									"name": "Destination",
									"value": "hana/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "SapOpenHubSource"
								},
								"sink": {
									"type": "BlobSink"
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_271",
									"type": "DatasetReference",
									"parameters": {
										"cw_openHubDestinationName": "@item().source.openHubDestinationName",
										"cw_excludeLastRequest": "@item().source.excludeLastRequest"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_271",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"openHubDestinationName": "DDATS",
							"excludeLastRequest": false
						},
						"destination": {
							"fileName": "DDATS.txt"
						}
					},
					{
						"source": {
							"openHubDestinationName": "OH1_DELTA",
							"excludeLastRequest": true
						},
						"destination": {
							"fileName": "OH1_DELTA.txt"
						}
					},
					{
						"source": {
							"openHubDestinationName": "OH_DSO",
							"excludeLastRequest": true
						},
						"destination": {
							"fileName": "OH_DSO.txt"
						}
					},
					{
						"source": {
							"openHubDestinationName": "OH_FLA",
							"excludeLastRequest": true
						},
						"destination": {
							"fileName": "OH_FLA.txt"
						}
					},
					{
						"source": {
							"openHubDestinationName": "TYPE_FL",
							"excludeLastRequest": true
						},
						"destination": {
							"fileName": "TYPE_FL.txt"
						}
					}
				]
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}
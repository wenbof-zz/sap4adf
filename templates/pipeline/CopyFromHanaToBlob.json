{
	"name": "CopyFromHanaToBlob",
	"properties": {
		"activities": [
			{
				"name": "HanaToBlob",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "RelationalSource",
						"query": "select \n\"LOGDATE\",\n\"LOGTIME\",\n\"LOGID\",\n\"TABNAME\",\n\"LOGKEY\",\n\"HOSTNAME\",\n\"USERNAME\",\n\"TCODE\",\n\"PROGNAME\",\n\"OPTYPE\",\n\"VERSNO\",\n\"LANGUAGE\",\n\"DATALN\"\n from \"SYSTEM\".\"DBTABLOG\""
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "HANASource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlob4HanaPerf",
						"type": "DatasetReference"
					}
				]
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}